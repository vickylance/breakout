{
  "name": "parcel-sandbox",
  "version": "1.0.0",
  "description": "Simple Parcel Sandbox",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "parcel build index.html --experimental-scope-hoisting -d ./docs",
    "serve": "http-server -p 4000 ./docs"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.6.4"
  },
  "license": "MIT"
}